#!/bin/bash
# Add persistent routes



sysctl -w net.ipv4.ip_forward=1
iptables -t nat -A POSTROUTING -o enp0s3 -j MASQUERADE
iptables -A FORWARD -i wlx7cc2c60a6366 -o enp0s3 -j ACCEPT
iptables -A FORWARD -i enp0s3 -o wlx7cc2c60a6366 -m state --state RELATED,ESTABLISHED -j ACCEPT
iptables -A FORWARD -i wlxa86e84a14458 -o enp0s3 -j ACCEPT
iptables -A FORWARD -i enp0s3 -o wlxa86e84a14458 -m state --state RELATED,ESTABLISHED -j ACCEPT
exit 0
